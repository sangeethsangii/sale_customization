<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Sample Submission -->
        <record id="seq_sample_submission" model="ir.sequence">
            <field name="name">Sample Submission</field>
            <field name="code">sample.submission</field>
            <field name="prefix">SAMPLE/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
        </record>

        <!-- Sequence for Material Lines -->
        <record id="seq_sample_material" model="ir.sequence">
            <field name="name">Sample Material</field>
            <field name="code">sample.material</field>
            <field name="prefix">MAT/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
        </record>
    </data>
</odoo>



