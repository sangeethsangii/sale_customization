<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Tree View -->
    <record id="view_checklist_master_tree" model="ir.ui.view">
        <field name="name">checklist.master.tree</field>
        <field name="model">checklist.master</field>
        <field name="arch" type="xml">
            <tree>

                <field name="name"/>

            </tree>
        </field>
    </record>

    <!-- Form View -->
    <record id="view_checklist_master_form" model="ir.ui.view">
        <field name="name">checklist.master.form</field>
        <field name="model">checklist.master</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>

                        <field name="name" required="1"/>

                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>




